{
	"easycom": {
		"autoscan": true,
		"custom": {
			"^wd-(.*)": "wot-design-uni/components/wd-$1/wd-$1.vue"
		}
	},
	"pages": [
		{
			"path": "pages/mine/index",
			"style": {
				"navigationBarTitleText": "个人中心",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/venue/index",
			"style": {
				"navigationBarTitleText": "找场馆",
				"navigationStyle": "custom"
			}
		},
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "uni-app"
			}
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"color": "#999999",
		"selectedColor": "#000000",
		"backgroundColor": "#ffffff",
		"list": [
			{
				"pagePath": "pages/index/index",
				"text": "闪动",
				"iconPath": "static/tabbar/flash.png",
				"selectedIconPath": "static/tabbar/flash-active.png"
			},
			{
				"pagePath": "pages/venue/index",
				"text": "找场馆",
				"iconPath": "static/tabbar/venue.png",
				"selectedIconPath": "static/tabbar/venue-active.png"
			},
			{
				"pagePath": "pages/index/index",
				"text": "俱乐部",
				"iconPath": "static/tabbar/club.png",
				"selectedIconPath": "static/tabbar/club-active.png"
			},
			{
				"pagePath": "pages/mine/index",
				"text": "我的",
				"iconPath": "static/tabbar/mine.png",
				"selectedIconPath": "static/tabbar/mine-active.png"
			}
		]
	}
}